{
    "name": "salesforce-pubsub-api-client",
    "version": "2.1.3",
    "type": "module",
    "description": "A node client for the Salesforce Pub/Sub API",
    "author": "pozil",
    "license": "CC0-1.0",
    "homepage": "https://github.com/pozil/pub-sub-api-node-client",
    "main": "src/client.js",
    "scripts": {
        "prettier": "prettier --write '**/*.{css,html,js,json,md,yaml,yml}'",
        "lint": "eslint .",
        "prepare": "husky install",
        "precommit": "lint-staged"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.8.0",
        "@grpc/proto-loader": "^0.7.4",
        "avro-js": "^1.11.1",
        "certifi": "^14.5.15",
        "dotenv": "^16.0.3",
        "jsforce": "^1.11.0",
        "node-fetch": "^3.3.0"
    },
    "devDependencies": {
        "eslint": "^8.31.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.1"
    },
    "lint-staged": {
        "**/*.{css,html,js,json,md,yaml,yml}": [
            "prettier --write"
        ],
        "**/src/**/*.js": [
            "eslint"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pozil/pub-sub-api-node-client"
    },
    "volta": {
        "node": "18.12.1",
        "npm": "8.19.2"
    },
    "keywords": [
        "salesforce",
        "pubsub",
        "api",
        "grpc"
    ],
    "files": [
        "src/*",
        "pubsub_api.proto"
    ]
}
